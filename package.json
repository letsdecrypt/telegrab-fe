{
  "name": "telegrab-fe",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "A Runtime Accelerated Rendering Infrastructure (Rari) application",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run typecheck && vite build",
    "dev": "vite",
    "predev": "pnpm run build",
    "start": "NODE_ENV=production rari start",
    "deploy:railway": "rari deploy railway",
    "deploy:render": "rari deploy render",
    "clean": "rm -rf dist",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "typecheck": "tsgo"
  },
  "dependencies": {
    "rari": "latest",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260109.1",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "rolldown-vite": "^7.3.1",
    "tailwindcss": "^4.1.18"
  }
}
